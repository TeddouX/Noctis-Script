obj Vec2 {
    pub Int x = 0;
    pub Int y = 0;

    pub Vec2(Int x, Int y) {
        this.x = x;
        this.y = y;
    }

    pub Int AddMembers() { return x + y; }
}

// 1 less than necessary to trigger the garbage collector
fun FunctionThatCreates9Objects() {
    Vec2 v1 = new Vec2(1,1);    
    Vec2 v2 = new Vec2(1,1);    
    Vec2 v3 = new Vec2(1,1);    
    Vec2 v4 = new Vec2(1,1);    
    Vec2 v5 = new Vec2(1,1);    
    Vec2 v6 = new Vec2(1,1);    
    Vec2 v7 = new Vec2(1,1);    
    Vec2 v8 = new Vec2(1,1);    
    Vec2 v9 = new Vec2(1,1);    
}

fun Main() {
    FunctionThatCreates9Objects();
    // Trigger the garbage collector
    Vec2 v1 = new Vec2(1,1);
    Vec2 v2 = new Vec2(1,1);
}
